#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/ts-node@10.0.0_@swc+core@1.4.12_@types+node@18.15.11_typescript@5.5.4/node_modules/ts-node/dist/node_modules:/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/ts-node@10.0.0_@swc+core@1.4.12_@types+node@18.15.11_typescript@5.5.4/node_modules/ts-node/node_modules:/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/ts-node@10.0.0_@swc+core@1.4.12_@types+node@18.15.11_typescript@5.5.4/node_modules:/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/ts-node@10.0.0_@swc+core@1.4.12_@types+node@18.15.11_typescript@5.5.4/node_modules/ts-node/dist/node_modules:/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/ts-node@10.0.0_@swc+core@1.4.12_@types+node@18.15.11_typescript@5.5.4/node_modules/ts-node/node_modules:/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/ts-node@10.0.0_@swc+core@1.4.12_@types+node@18.15.11_typescript@5.5.4/node_modules:/Users/adrielavila/Documents/proyectos/jose/candidate-evaluator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
else
  exec node  "$basedir/../ts-node/dist/bin-transpile.js" "$@"
fi
